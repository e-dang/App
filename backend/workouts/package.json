{
  "name": "workouts",
  "version": "0.0.1",
  "description": "",
  "author": "Eric Dang",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:dev": "docker build -t workouts:dev -f docker/dev/Dockerfile .",
    "build:prod": "docker build -t workouts:prod -f docker/prod/Dockerfile .",
    "precompile": "rimraf dist",
    "compile": "nest build",
    "up:dev": "docker compose -f docker/dev/docker-compose.yaml -p dev up",
    "up:test": "docker compose -f docker/test/docker-compose.yaml -p test up",
    "up:intercept": "telepresence intercept workouts --port 3000 --docker-run --docker-mount=/secrets -- -v $(pwd):/home/workouts/app -v /home/workouts/app/node_modules -it --rm ericdang/workouts:dev /bin/sh",
    "up:prod": "docker run -it --rm --name workouts -p 3000:3000 -v /home/service/app/node_modules -v /home/service/app/dist -v $(pwd):/home/service/app ericdang/workouts:prod /bin/sh",
    "start": "nest start",
    "start:shell": "docker exec -it dev-workouts-1 /bin/sh",
    "start:dev": "docker exec -it dev-workouts-1 yarn nest start --watch",
    "start:debug": "docker exec -it dev-workouts-1 yarn nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "docker exec -it test-workouts-1 yarn jest --config=jest.config.int.js",
    "test:watch": "docker exec -it test-workouts-1 yarn jest --watch",
    "test:cov": "docker exec -it test-workout-1 yarn jest --coverage",
    "test:debug": "docker exec -it test-workout-1 node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migration:generate": "typeorm migration:generate --config dist/src/ormconfig.js -d src/migrations",
    "migration:run": "typeorm migration:run --config dist/src/ormconfig.js",
    "clean:dev:app": "docker container rm dev-workouts-1 && docker volume rm dev_node_modules dev_dist",
    "clean:dev:db": "docker container rm dev-postgres-1 && docker volume rm dev_postgres",
    "clean:test:app": "docker container rm test-workouts-1 && docker volume rm test_node_modules test_dist",
    "clean:test:db": "docker container rm test-postgres-1 && docker volume rm test_postgres",
    "lint": "eslint \"{src,__tests__,config,scripts}/**/*.ts\"",
    "generate:dev:env": "ts-node scripts/gen-dev-env.ts",
    "generate:keys": "npx ts-node scripts/genKeys.ts",
    "generate:token": "npx ts-node -r tsconfig-paths/register scripts/genToken.ts"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/terminus": "^8.0.4",
    "@nestjs/typeorm": "^8.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "express": "^4.17.2",
    "jose": "^4.3.8",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.178",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-edang": "^1.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mockdate": "^3.0.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.5.5",
    "typescript-transform-paths": "^3.3.1"
  }
}
